    <tool name="SampleLandcoverOrthogonalMethod" displayname="Step 5B: Sample Landcover - Orthogonal Method"
          toolboxalias="TTools" xmlns="">
        <arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath>
        <parameters>
            <param name="nodes_fc" displayname="Stream Nodes Feature Class" type="Required" direction="Input"
                   datatype="Feature Class" expression="nodes_fc">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;File
                    path to the stream nodes feature class.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="start_bank" displayname="Start at Stream Bank" type="Required" direction="Input"
                   datatype="Boolean" expression="start_bank">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;Checkbox
                    to indicate if the transect should start at the stream bank. If unchecked, the transect will start
                    at the stream node, if checked, the transect will start at the stream bank. This box is typically
                    checked.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="transsample_count" displayname="Samples per Transect" type="Required" direction="Input"
                   datatype="Long" expression="transsample_count">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;Number
                    of samples per transect. The number does not include the sample at the stream node.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="transsample_distance" displayname="Distance Between Samples (m)" type="Required"
                   direction="Input" datatype="Double" expression="transsample_distance">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The
                    distance between transect samples measured in meters.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="lc_raster" displayname="Landcover Raster" type="Required" direction="Input"
                   datatype="Raster Dataset" expression="lc_raster">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;File
                    path to the landcover code, landcover height, or landcover elevation raster.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="lc_units" displayname="Landcover Units" type="Required" direction="Input" datatype="String"
                   expression="Meters | Feet | None">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The
                    unit of the landcover raster (a.k.a. the unit of height or elevation). Options include "Feet",
                    "Meters", or "None". Use “None” if the landcover raster cell values are codes and do not represent
                    elevation or height units&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="z_raster" displayname="Elevation Raster" type="Required" direction="Input"
                   datatype="Raster Dataset" expression="z_raster">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;P STYLE="font-size:16ptmargin:0 0 8 0;"&gt;&lt;SPAN&gt;File
                    path to the elevation raster.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="z_units" displayname="Elevation Units" type="Required" direction="Input" datatype="String"
                   expression="Meters | Feet">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The
                    elevation raster units are specified here. Either "Feet" or "Meters". If the unit is not in feet or
                    meters, the elevation values must be converted prior to tool input.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="lc_point_fc" displayname="Output Landcover Points" type="Required" direction="Output"
                   datatype="Feature Class" expression="lc_point_fc">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;File
                    path and name of output landcover point feature class. This feature identifies the location,
                    landcover height, and canopy cover information for each landcover sample point.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="block_size" displayname="Block Size (km)" type="Required" direction="Input" datatype="Double"
                   expression="block_size">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The
                    length and width dimensions in kilometers for the square blocks of elevation raster cells pulled
                    into an array. The suggested default value is 10. If an error occurs, try reducing the block size. To
                    increase processing speed, the elevation raster is subdivided iteratively into smaller blocks and
                    pulled into arrays for processing. Very large block sizes may use a lot of memory and result in an
                    error.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="overwrite_data" displayname="Overwrite Existing Data" type="Required" direction="Input"
                   datatype="Boolean" expression="overwrite_data">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;P STYLE="font-size:16ptmargin:0 0 8 0;"&gt;&lt;SPAN&gt;Checkbox
                    to indicate whether existing landcover data in the stream nodes feature class and in the landcover
                    point feature class can be overwritten.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
        </parameters>
    </tool>
