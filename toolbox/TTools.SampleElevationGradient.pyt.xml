    <tool name="SampleElevationGradient" displayname="Step 3: Sample Elevation Gradient" toolboxalias="TTools"
          xmlns="">
        <arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath>
        <parameters>
            <param name="nodes_fc" displayname="Stream Nodes Feature Class" type="Required" direction="Input"
                   datatype="Feature Layer" expression="nodes_fc">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;File
                    path to the stream nodes feature class output from TTools Step 1.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="searchCells" displayname="Search Radius (cells)" type="Required" direction="Input"
                   datatype="Long" expression="searchCells">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The
                    number of raster cells to search around the node for the lowest elevation.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="smooth_flag" displayname="Smooth Flat or Negative Gradients" type="Required" direction="Input"
                   datatype="Boolean" expression="smooth_flag">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;Checkbox
                    to indicate if smoothing should occur when the gradient is less than zero. When gradients are less
                    than zero, the algorithm moves downstream until a positive gradient is found and then recalculates
                    the gradient over the longer distance and applies the resulting value to all nodes over that
                    distance.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="z_raster" displayname="Elevation Raster" type="Required" direction="Input"
                   datatype="Raster Layer" expression="z_raster">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;File
                    path to the elevation raster.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="z_units" displayname="Elevation Units" type="Required" direction="Input" datatype="String"
                   expression="Meters | Feet">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The elevation raster units are specified here. Either "Feet"
                    or "Meters". If the z unit is not in feet or meters, the elevation values must be converted prior to
                    tool input.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="block_size" displayname="Block Size (km)" type="Required" direction="Input" datatype="Double"
                   expression="block_size">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The length and width dimensions in kilometers for the square
                    blocks of elevation raster cells pulled into an array. The suggested default value is 10. If an error
                    occurs, try reducing the block size. To increase processing speed, the elevation raster is
                    subdivided iteratively into smaller blocks and pulled into arrays for processing. Very large block
                    sizes may use a lot of memory and result in an error.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="overwrite_data" displayname="Overwrite Existing Elevation/Gradient Fields" type="Required"
                   direction="Input" datatype="Boolean" expression="overwrite_data">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;Checkbox
                    to indicate if existing elevation and gradient data in the stream nodes feature class can be
                    overwritten.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
        </parameters>
    </tool>
