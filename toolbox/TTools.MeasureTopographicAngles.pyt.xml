    <tool name="MeasureTopographicAngles" displayname="Step 4: Measure Topographic Angles" toolboxalias="TTools"
          xmlns="">
        <arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath>
        <parameters>
            <param name="nodes_fc" displayname="Stream Nodes Feature Class" type="Required" direction="Input"
                   datatype="Feature Layer" expression="nodes_fc">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;File path to the stream nodes feature class. &lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="topo_directions" displayname="Topographic Direction Option" type="Required" direction="Input"
                   datatype="Long" expression="1 | 2 | 3">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;An integer value corresponding to a specific list of azimuth
                    directions to sample. There are three options available via a drop-down menu. The options are:&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;P
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;1: Azimuth directions used by the Heat Source model or
                    Washington Department of Ecology's Shade model. The topographic shade angles are sampled in 3
                    azimuth directions (west, south, and east: [270&lt;/SPAN&gt;&lt;SPAN&gt;°, 180°, 90&lt;/SPAN&gt;&lt;SPAN&gt;&lt;SPAN&gt;°]).&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;P
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;2: Topographic shade angles are sampled in all cardinal and
                    intercardinal azimuth directions: [45°, 90&lt;/SPAN&gt;&lt;SPAN&gt;°, 135°, 180&lt;/SPAN&gt;&lt;SPAN&gt;°,
                    225°, 270&lt;/SPAN&gt;&lt;SPAN&gt;°, 315°, 360°].&lt;/SPAN&gt;&lt;/P&gt;&lt;P STYLE="font-size:16pt"&gt;&lt;SPAN&gt;3:
                    Azimuth directions used by the CE-QUAL-W2 model. Topographic shade angles are sampled in 20 degree&lt;/SPAN&gt;&lt;SPAN&gt;&lt;SPAN&gt;
                    increments:[0&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;SPAN&gt;°, 20°, 40&lt;/SPAN&gt;&lt;SPAN&gt;°, 60°, 80&lt;/SPAN&gt;&lt;SPAN&gt;°,
                    100°, 120&lt;/SPAN&gt;&lt;SPAN&gt;&lt;SPAN&gt;°, 140&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;SPAN&gt;°, 160&lt;/SPAN&gt;&lt;SPAN&gt;&lt;SPAN&gt;°,
                    180&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;SPAN&gt;°, 200°, 220&lt;/SPAN&gt;&lt;SPAN&gt;°, 240°, 260&lt;/SPAN&gt;&lt;SPAN&gt;°,
                    280°, 300&lt;/SPAN&gt;&lt;SPAN&gt;°, 320°, 340&lt;/SPAN&gt;&lt;SPAN&gt;°]. When option 3 is selected
                    the output topographic angles are converted from degrees to radians as the CE-QUAL-W2 model uses
                    radians for model input.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN /&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="searchDistance_max_km" displayname="Max Search Distance (km)" type="Required" direction="Input"
                   datatype="Double" expression="searchDistance_max_km">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The
                    maximum distance in kilometers to search for the largest topographic shade angle in each azimuth
                    direction.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="z_raster" displayname="Elevation Raster" type="Required" direction="Input"
                   datatype="Raster Layer" expression="z_raster">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;File
                    path to the elevation raster.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="z_units" displayname="Elevation Units" type="Required" direction="Input" datatype="String"
                   expression="Meters | Feet">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;The
                    elevation raster units are specified here. Either "Feet" or "Meters". If the unit is not in feet or
                    meters, the elevation values must be converted prior to tool input&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="topo_fc" displayname="Output Topographic Feature Class" type="Required" direction="Output"
                   datatype="Feature Class" expression="topo_fc">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;Path
                    and name of output topographic point feature. This feature identifies the location on the elevation
                    raster producing the largest topographic shade angle for each node and sample direction.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="block_size" displayname="Raster Block Size (km)" type="Required" direction="Input"
                   datatype="Double" expression="block_size">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN
                    STYLE="font-size:16pt"&gt;&lt;SPAN&gt;The length and width dimensions in kilometers for the square
                    blocks of elevation raster cells pulled into an array. The suggested default value is 10. If an error
                    occurs, try reducing the block size. To increase processing speed, the elevation raster is
                    subdivided iteratively into smaller blocks and pulled into arrays for processing. Very large block
                    sizes may use a lot of memory and result in an error.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
            <param name="overwrite_data" displayname="Overwrite Existing Data" type="Required" direction="Input"
                   datatype="Boolean" expression="overwrite_data">
                <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN STYLE="font-size:16pt"&gt;&lt;SPAN&gt;Checkbox
                    to indicate if existing topographic shade angle data in the stream nodes feature class and in the
                    topographic points feature class can be overwritten.&lt;/SPAN&gt;&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;
                </dialogReference>
            </param>
        </parameters>
    </tool>
